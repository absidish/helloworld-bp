{
  "name": "Hello World web page",
  "description": "no need for desc",
  "nodes":[
    {
      "name":"web-apache",
      "template":"apache@b47933477ca87eacacf0287c0dcc7637a5bbc4df",
      "peer_group":"free peers",
      "size":"TINY",
      "expose":[
        {
          "protocol":"http",
          "domain":"amamutov.envs.devsubutaihub.com",
          "port_ext":"80",
          "port_int":"80"
        }
      ]
    }
  ],
  "peerGroups":[
    {
      "name": "free peers",
      "pricingPref": { "maxPrice": "0" }
    }
  ],
  "configuration": {
    "arguments": [
      {
        "name": "htmlFileName",
        "value": "${filename}"
      },
      {
        "name": "htmlContent",
        "value": "Hello world! This page was craeted with Subutai blueprint: https://github.com/samsonbek/helloworld-bp/"
      }
    ],
    "playbook": "https://github.com/samsonbek/helloworld-bp/archive/master.zip",
    "script": "main.yml",
    "groups": [
      {
        "name": "web-group",
        "nodes_name": [ "web-apache" ]
      }
    ]
  }
}
